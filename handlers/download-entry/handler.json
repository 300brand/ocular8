{
	"Command": [
		"./download-entry",
		"-mongo",
		"{{ .Config.Mongo }}",
		"-nsqdhttp",
		"{{ .Config.NsqdHTTP }}",
		"{{ .Data }}"
	],
	"NSQ": {
		"Consume": {
			"Topic":      "entry.id.download",
			"Channel":    "download-entry",
			"Concurrent": 20
		},
		"Produce": [
			"article.id.extract.goose"
		]
	},
	"Name": "Download Feed Entry"
}
