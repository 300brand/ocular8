{
	"Command": [
		"./download-feed",
		"-mongo",
		"{{ .Config.Mongo }}",
		"-nsqdhttp",
		"{{ .Config.NsqdHTTP }}",
		"{{ .Data }}"
	],
	"NSQ": {
		"Consume": {
			"Topic":      "feed.id.download",
			"Channel":    "download-feed",
			"Concurrent": 2
		},
		"Produce": [
			"article.json.download"
		]
	},
	"Name": "Download Feeds"
}
