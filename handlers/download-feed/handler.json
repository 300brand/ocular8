{
	"Command": [
		"./download-feed",
		"-mongo",
		"{{ .Config.Mongo }}",
		"-nsqdhttp",
		"{{ .Config.NsqdHTTP }}",
		"{{ .Data }}"
	],
	"NSQ": {
		"Consume": {
			"Topic":   "feed.id.download",
			"Channel": "download-feed"
		},
		"Produce": [
			"article.url.download"
		]
	},
	"Name": "Download Feeds"
}
